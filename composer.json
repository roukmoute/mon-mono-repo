{
    "name": "roukmoute/mon-mono-repo",
    "description": "Test Mono Repo",
    "type": "library",
    "require": {
        "php": "^7.2",
        "symfony/dependency-injection": "^5.0",
        "symfony/http-kernel": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symplify/monorepo-builder": "^9.4"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "YourMonorepo\\FirstPackage\\": "packages/first-package/src",
            "YourMonorepo\\SecondPackage\\": "packages/second-package/src"
        }
    },
    "authors": [
        {
            "name": "Mathias STRASSER",
            "email": "contact@roukmoute.fr"
        }
    ],
    "replace": {
        "your-monorepo/first-package": "self.version",
        "your-monorepo/second-package": "self.version"
    }
}
